{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js && npm run lint:code",
    "typeCheck": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.ts .",
    "lint:js:fix": "eslint --fix --ext .js,.ts .",
    "lint:code": "prettier .",
    "lint:code:fix": "prettier --write .",
    "lint:fix": "npm run lint:js:fix && npm run lint:code:fix",
    "generate:client": "rm -rf apiClient && openapi-generator-cli generate -g typescript-axios -i ../backend/docs/openAPISchema.yaml -o apiClient",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "@types/node": "18.16.12",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "axios": "^1.4.0",
    "framer-motion": "^10.12.16",
    "next": "13.4.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.3.2",
    "@storybook/nextjs": "^7.3.2",
    "@storybook/react": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/axios": "^0.14.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.0.1",
    "eslint-config-next": "13.4.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-storybook": "^0.6.13",
    "prettier": "^2.3.2",
    "storybook": "^7.3.2",
    "storybook-addon-next": "^1.8.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
